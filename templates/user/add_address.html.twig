{% extends 'base.html.twig' %}

{% block title %}Mon profil{% endblock %}

{% block body %}
<div class="cont">
    <h1 class="title-h1 title-center">Mon adresse</h1>
    <div class="form">
        {{ form_start(add_address_form) }}
            <div class="form-group">
                {{ form_row(add_address_form.street) }}
            </div>
            <div class="form-group">
                {{ form_row(add_address_form.postalCode) }}
            </div>
            <div class="form-group">
                {{ form_row(add_address_form.city) }}
            </div>
            <div class="form-group">
                {{ form_row(add_address_form.country) }}
            </div>
            <div class="form-submit">
                {{ form_row(add_address_form.submit) }}
            </div>
        {{ form_end(add_address_form) }}
    </div>
</div>
{% endblock %}